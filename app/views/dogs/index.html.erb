<p id="notice"><%= notice %></p>

<h1>All Dogs/Users</h1>

<table>
  <thead>
    <tr>
      <th>Dog</th>
      <th>Age</th>
      <th>Breed</th>
      <th>Email</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dogs.each do |dog| %>
      <tr>
        <td><%= dog.name %></td>
        <td><%= dog.age %></td>
        <td><%= dog.breed %></td>
        <td><%= dog.email %></td>

      </tr>
    <% end %>
  </tbody>
</table>


  